	this.ie = this.ff = this.chrome = this.opera = this.safari = false;
	this.setBrowserInfo();
	this.classAttribute = (((this.ie && this.doc.compatMode == 'BackCompat') || (this.ie && this.ieVersion <= 7)) ? 'className' : 'class');
