-- Get Individual Name & Birth Details --
TblDetails = {}																	-- Save details in table cache for quick subsequent lookup
function TblIndividualDetails(ptrIndi,intRecId)
	if not TblDetails[intRecId] then
		TblDetails[intRecId] = {}
		local tblDetails = TblDetails[intRecId]
		tblDetails.First = fhGetItemText(ptrIndi,"~.NAME:FIRST")										-- 07 June 2013
		tblDetails.Given = encoder.StrCP1252_URI(fhGetItemText(ptrIndi,"~.NAME:GIVEN_ALL"))		-- 07 June 2013
		tblDetails.Sname = encoder.StrCP1252_URI(fhGetItemText(ptrIndi,"~.NAME:SURNAME"))		-- 07 June 2013
		tblDetails.Place = encoder.StrCP1252_URI(fhGetItemText(ptrIndi,"~.BIRT.PLAC"))			-- 07 June 2013
		tblDetails.Birth = fhCallBuiltInFunction("EstimatedBirthDate",ptrIndi,"MID",9):GetYear()
		if tblDetails.Birth == 0 then
			tblDetails.Birth = ""												-- No estimated Birth year available
			tblDetails.Range = ""
		else																		-- Tolerance range = latest Birth year - earliest Birth year
			tblDetails.Range = fhCallBuiltInFunction("EstimatedBirthDate",ptrIndi,"LATEST",9):GetYear() - fhCallBuiltInFunction("EstimatedBirthDate",ptrIndi,"EARLIEST",9):GetYear()
			tblDetails.Range = math.ceil( tblDetails.Range / 2 ) + 5	-- 10 June 2013 extend range by ±5 years to allow for approx/calc/est, 1841 Census rounding, etc -- previously math.max( 5, math.ceil( tblDetails.Range / 2 ) )
		end
	end
	return TblDetails[intRecId]
end -- function TblIndividualDetails

-- Build Ancestry Co Search --
function StrAncestryCo(strWebAddress,tblCensusData,ptrIndi,tblDetails,strSurname,ptrFamily)			-- 25 Nov 2013 V3.0
	local strAncestryCo = ""
	if strWebAddress then
		if not ptrIndi then 													-- Build table heading
			strAncestryCo = "\n						<th>"..strWebAddress.."</th>"
		else																		-- Build table data search anchor tag
			strAncestryCo = StrAncestryCoTemplate
			strAncestryCo = strAncestryCo:replace("{WebAddress}",strWebAddress)
			strAncestryCo = strAncestryCo:replace("{CensusCode}",tblCensusData.Census)					-- 25 Nov 2013 V3.0
			strAncestryCo = strAncestryCo:replace("{GivenNames}",tblDetails.Given)
			strAncestryCo = strAncestryCo:replace("{TheSurname}",encoder.StrCP1252_URI(strSurname))	-- 07 June 2013
			strAncestryCo = strAncestryCo:replace("{Birth_Year}",tblDetails.Birth)
			strAncestryCo = strAncestryCo:replace("{BirthRange}",tblDetails.Range)
			strAncestryCo = strAncestryCo:replace("{BirthPlace}",tblDetails.Place)
			strAncestryCo = strAncestryCo:replace("{CensusYear}",tblCensusData.Year)						-- 25 Nov 2013 V3.0
			local tblExcludeSex = { Male="f", Female="m", [""]="" }
			strAncestryCo = strAncestryCo:replace("{ExcludeSex}",tblExcludeSex[fhGetItemText(ptrIndi,"~.SEX")])
			local strRelatives = ""
			local strRelation = ""
			local tblRelation = {}
			local ptrRelation = fhNewItemPtr()
			ptrRelation:MoveTo(ptrIndi,"~.~FATH>")							-- Get Father
			if ptrRelation:IsNotNull() then
				tblRelation = TblIndividualDetails(ptrRelation,fhGetRecordId(ptrRelation))
				strRelation = StrAncestryFatherTemplate
				strRelation = strRelation:replace("{GivenNames}",tblRelation.Given)
				strRelation = strRelation:replace("{TheSurname}",tblRelation.Sname)
				strRelatives = strRelatives..strRelation
			end
			ptrRelation:MoveTo(ptrIndi,"~.~MOTH>")							-- Get Mother
			if ptrRelation:IsNotNull() then
				tblRelation = TblIndividualDetails(ptrRelation,fhGetRecordId(ptrRelation))
				strRelation = StrAncestryMotherTemplate
				strRelation = strRelation:replace("{GivenNames}",tblRelation.Given)
				strRelation = strRelation:replace("{TheSurname}",tblRelation.Sname)
				strRelatives = strRelatives..strRelation
			end
			if ptrFamily:IsNotNull() then									-- Get Spouse at time of Census
				if fhGetItemText(ptrIndi,"~.SEX") == "Female" then
					ptrRelation:MoveTo(ptrFamily,"~.HUSB>")
				else
					ptrRelation:MoveTo(ptrFamily,"~.WIFE>")
				end
				if ptrRelation:IsNotNull() then
					tblRelation = TblIndividualDetails(ptrRelation,fhGetRecordId(ptrRelation))
					strRelation = StrAncestrySpouseTemplate
					strRelation = strRelation:replace("{GivenNames}",tblRelation.Given)
					strRelation = strRelation:replace("{TheSurname}",tblRelation.Sname)
					strRelatives = strRelatives..strRelation
				end
			end
			strAncestryCo = strAncestryCo:replace("{Relatives}",strRelatives)
		end
	end
	return strAncestryCo
end -- function StrAncestryCo

-- Build Find My Past Search --
function StrFindMyPast(strWebAddress,tblCensusData,ptrIndi,tblDetails,strSurname)						-- 25 Nov 2013 V3.0
	local strFindMyPast = ""
	if strWebAddress then
		local intCensusYear = tblCensusData.Year																	-- 25 Nov 2013 V3.0
		-- 2 April 2014 V3.1 -- ALL FindMyPast web sites now all use same World Search format
		if not ptrIndi then 													-- Build table heading
			strFindMyPast = "\n						<th>"..strWebAddress.."</th>"
		else																		-- Build table data search anchor tag
			local intBirth_Year = tblDetails.Birth
			local intBirthRange = tblDetails.Range
			if intBirthRange > 5 and intBirthRange < 40 then
				intBirthRange = math.ceil(intBirthRange/10) * 10		-- Round up range offset to ±10, ±20, or ±40								-- 13 June 2013
				if intBirthRange == 30 then intBirthRange = 40 end
			end
			local strCategory = "substitutes"								-- For "findmypast.com" U.S.A. and "findmypast.ie" Ireland			-- 13 June 2013
			if strWebAddress == "new.findmypast.co.uk"
			or strWebAddress == "findmypast.com.au" then
				strCategory = "surveys"										-- For "new.findmypast.co.uk" U.K. and "findmypast.com.au" Australia	-- 13 June 2013
			end
			strFindMyPast = StrFindMyPastTemplate
			strFindMyPast = strFindMyPast:replace("{WebAddress}",strWebAddress)
			strFindMyPast = strFindMyPast:replace("{RegionCode}",tblCensusData.Region)							-- 13 June 2013 & 25 Nov 2013 V3.0
			strFindMyPast = strFindMyPast:replace("{Category}"  ,strCategory)										-- 13 June 2013
--!			strFindMyPast = strFindMyPast:replace("{RecordCode}",tblCensusData.Record)							-- 13 June 2013 & 25 Nov 2013 V3.0 -- 2 April 2014 V3.1 discontinued
			strFindMyPast = strFindMyPast:replace("{CensusYear}",intCensusYear)
			strFindMyPast = strFindMyPast:replace("{GivenNames}",encoder.StrCP1252_URI(tblDetails.First))	-- 07 June 2013
			strFindMyPast = strFindMyPast:replace("{TheSurname}",encoder.StrCP1252_URI(strSurname))			-- 07 June 2013
--	?		strFindMyPast = strFindMyPast:replace("{FirstName}" ,"")	-- Another household member name could be added
--	?		strFindMyPast = strFindMyPast:replace("{LastName}"  ,"")	-- See StrFindMyPastTemplate "member..." filters
			strFindMyPast = strFindMyPast:replace("{Birth_Year}",intBirth_Year)
			strFindMyPast = strFindMyPast:replace("{BirthRange}",intBirthRange)
		end
	end
	return strFindMyPast
end -- function StrFindMyPast

-- Current Family At Census --
function CurrentFamily(ptrIndi,datCensusDate)
	local ptrFamily  = fhNewItemPtr()
	local ptrMarried = fhNewItemPtr()										-- 14 July 2013
	local ptrFamsTag = fhNewItemPtr()										-- Find Marriage before Census Date
	local ptrFamLink = fhNewItemPtr()
	local ptrFamDate = fhNewItemPtr()
	local datFamDate = fhNewDate()
	local strSurname = fhGetItemText(ptrIndi,"~.NAME:SURNAME")		-- 07 June 2013
	ptrFamsTag:MoveTo(ptrIndi,"~.FAMS")										-- First Family As Spouse
	while ptrFamsTag:IsNotNull() do
		ptrFamLink = fhGetValueAsLink(ptrFamsTag)
		ptrFamDate:MoveTo(ptrFamLink,"~.MARR.DATE")
		if ptrFamDate:IsNull() then											-- If no Marriage Date use Birth of First Child
			ptrFamDate:MoveTo(ptrFamLink,"~.CHIL>BIRT.DATE")
		end
		datFamDate = fhGetValueAsDate(ptrFamDate)
		if not(datFamDate:IsNull()) and datFamDate:Compare(datCensusDate) <= 0 then
			strStatus = fhGetItemText(ptrFamLink,"~._STAT")
			if strStatus ~= "Never Married" and strStatus ~= "Unmarried Couple" then
				ptrFamily  = ptrFamLink:Clone()								-- Family couple married or with child before Census Date
				ptrMarried = ptrFamDate:Clone()								-- 14 July 2013
			end
		end
		if fhGetItemText(ptrIndi,"~.SEX") == "Female" and ptrFamily:IsNotNull() then -- 19 July 2013 moved from after 'end'
			local strHusband = fhGetItemText(ptrFamily,"~.HUSB>NAME:SURNAME") -- 07 June 2013
			if strHusband ~= "" then strSurname = strHusband end		-- Married women use husband's surname, if any -- 14 July 2013
		end
		ptrFamsTag:MoveNext("SAME_TAG")
	end
	return ptrFamily,strSurname,ptrMarried -- ptrFamDate				-- 14 July 2013
end -- function CurrentFamily

-- Get List of Records to Search --
function TblRecordList(tblRecordList,strRecordList)
	if strRecordList then														-- Ancestors or Descendants or Relations needed
		local ptrRoot = tblRecordList[1]
		local isAncestor   = strRecordList:match("Ancestor")			-- Ancestors    or  Ancestors & Descendants ?
		local isDescendant = strRecordList:match("Descendant")			-- Descendants  or  Ancestors & Descendants ?
		local isRelation   = strRecordList:match("Relation")			-- Relations ?
		local intRelation  = fhCallBuiltInFunction("RelationPool",ptrRoot) or 0
		local intIndi = 0
		for ptrIndi in iterate.Records("INDI") do							-- Count number of Individuals
			intIndi = intIndi + 1
		end
		if intIndi >= 1000 then
			progbar.Start('Finding Relations',intIndi)					-- Optionally start the Progress Bar
		end
		intIndi = 0
		for ptrIndi in iterate.Records("INDI") do							-- Loop through all Individuals to find relatives
			intIndi = intIndi + 1
			if isAncestor   and fhCallBuiltInFunction("IsAncestorOf"  ,ptrIndi,ptrRoot)
			or isDescendant and fhCallBuiltInFunction("IsDescendantOf",ptrIndi,ptrRoot)
			or isRelation   and fhCallBuiltInFunction("IsRelativeOf"  ,ptrIndi,ptrRoot)	-- 20 July 2013 alternative to RelationPool but only blood relations & spouses
			or isRelation   and fhCallBuiltInFunction("RelationPool"  ,ptrIndi) == intRelation then
				table.insert(tblRecordList,ptrIndi:Clone())				-- Ancestor or Descendant or Relation found
				progbar.Message("Record Id "..fhGetRecordId(ptrIndi))
				progbar.Step(intIndi)
				intIndi = 0
			end
			if progbar.Stop() then return end
		end
		progbar.Close()
	end
	return tblRecordList
end -- function TblRecordList

-- Check if Census is missing and Individual was in Census Country on Census Date -- 25 Nov 2103 V3.0
-- Many U.K. & Australia/Canada Census Dates were in same Year, some identical Dates and some different Dates, so must cater for all cases
TblFacts = {}																	-- Save subset of Facts in table cache for quick subsequent lookup
function IsCensusMissing(ptrIndi,intRecId,tblCensusData,isInCountry)
	if TblFacts.RecId ~= intRecId then										-- Start empty table when Record Id changes
		TblFacts = {}
		TblFacts.RecId = intRecId
		for ptrFact in iterate.Facts(ptrIndi) do							-- Save CENSus Events &/or all Facts with both full Simple Date and Place fields
			local strTag = fhGetTag(ptrFact)
			local datDate = fhGetValueAsDate(fhGetItemPtr(ptrFact,"~.DATE"))
			local strDate = datDate:GetDisplayText("COMPACT")			-- Compact Simple Date Format is "dd mmm yyyy" and all parts needed, so length is > 9
			local strPlace = fhGetItemText(ptrFact,"~.PLAC")
			if strTag == "CENS"
			or isInCountry and datDate:GetType() == "Simple" and datDate:GetSubtype() == "" and #strDate > 9 and #strPlace > 1 then
				table.insert(TblFacts,{ Tag=strTag, Date=datDate:Clone(), Place=", "..strPlace..", " })
			end
		end
	end
	isInCountry = true															-- Assume person is in Census Country on Census Date and missing Census Event
	for intFact, tblFact in ipairs(TblFacts) do							-- Check each Fact in lookup table
		local intCompare = tblFact.Date:Compare(tblCensusData.Date)	-- Comparison = 0 even if only partial match for CENSus Event Year or Month & Year
		if intCompare == 0 then												-- Fact Date matches Census Date
			local strTag = tblFact.Tag
			local strPlace = tblFact.Place
			if strTag == "CENS" and strPlace == ", , " then				-- CENSus Event for Date but no Place field
				return false														-- Assume it is for Census Country, so Census is NOT missing
			end
			for _, strCountry in ipairs ( tblCensusData.Country:split() ) do
				if strPlace:matches(", "..strCountry..", ") then
					if strTag == "CENS" then return false end				-- CENSus Event for Date & Country, so Census is NOT missing
				else
					isInCountry = false											-- Fact for Census Date but not Country, so Census is NOT missing as person was abroad
				end
			end
		end
		if intCompare > 0 then break end									-- Curtail loop when Fact Date is after Census Date
	end
	return isInCountry															-- If in Country on Date then Census is missing, otherwise Census is NOT missing
end -- function IsCensusMissing

-- Delete files accessed more than 1 hour ago --
function DeleteOldFiles(strFolder,strFile)
	local intTime = os.time() - 3600										-- Time in seconds one hour ago
	for strEntry in lfs.dir(strFolder) do									-- Search for files in the folder
		if strEntry ~= "." and strEntry ~= ".." then
			local strPath = strFolder.."\\"..strEntry
			local tblAttr, strError = lfs.attributes(strPath)			-- Obtain file attributes
			if not tblAttr then tblAttr = { mode="attrfail", error=strError } end 
			if tblAttr.mode == "file"
			and strEntry:matches(strFile) then								-- Simple file with matching name?
				if tblAttr.access < intTime then
					general.DeleteFile(strPath)								-- Delete if access time is an hour ago
				end
			end
		end
	end
end -- function DeleteOldFiles

-- Run Census Lookup --
function RunLookup(strCensusList,strAncestryCo,strFindMyPast,strRecordList,isInCountry)
	-- strCensusList	Chosen set of Census entries
	local strFolder = os.getenv("TEMP").."\\"								-- Define temporary folder for browser files
	local strPlugin = iup_gui.Plugin:lower():gsub(" ","")				-- Define lowercase and despaced Plugin name
	local strFile_js = strPlugin..".js" 									-- Define temporary filenames for .css, .js & .html files
	local strFile_css = strPlugin..".css"
	local strFile_html = os.tmpname():gsub("^\\","")..strPlugin..".html"
	DeleteOldFiles(strFolder,strPlugin)										-- Delete any old copies of the temporary files
	local strFileName = ""
	local strWebPage = StrWebPageTemplate									-- Web page of headings and placeholders for following components
	local tblWebGrid = {}														-- Web grid of Census web lines
	local strWebLine = ""														-- Web line in grid above
	local strLineColour = ""													-- Web line colour switch
	local tblCensusData = TblCensusData(strCensusList)					-- Table of Census data per year for chosen country
	local strTitleName = "selected Individuals"							-- Title name for Individuals via Select Records
	local tblRecordList = {}													-- List of Individual Records to search
	local intRecordList = 9000												-- Allow large numer of Individuals via Select Records
	if strRecordList then intRecordList = 1 end							-- But only one for Ancestors/Descendants/Relations option
	tblRecordList = fhPromptUserForRecordSel("INDI",intRecordList)	-- Select Records dialogue
	if #tblRecordList == 0 then return end
	if strRecordList then														-- Title details of chosen root person for Ancestors/Descendants/Relations option
		strTitleName = strRecordList..fhGetItemText(tblRecordList[1],"~.NAME").." "..fhCallBuiltInFunction("LifeDates",tblRecordList[1])
	end
	strTitleName = strCensusList.." are missing for "..strTitleName	-- Prefix Census List to title
	strWebPage = strWebPage:replace("{JS_File}",strFile_js)			-- Insert web page components into placeholders
	strWebPage = strWebPage:replace("{CSS_File}",strFile_css)
	strWebPage = strWebPage:replace("{TitleName}",encoder.StrCP1252_XML(strTitleName)) -- 07 June 2013
	strWebPage = strWebPage:replace("{AncestryCo}",StrAncestryCo(strAncestryCo))
	strWebPage = strWebPage:replace("{FindMyPast}",StrFindMyPast(strFindMyPast,tblCensusData[1])) -- 25 Nov 2103 V3.0
	local datBirthDate = fhNewDate()
	local datDeathDate = fhNewDate()
	local datCensusDate = fhNewDate()
	local intCensusYear = 0
	local tblIndiItem = {}														-- Result Set tables
	local tblRecordId = {}
	progbar.Setup(iup_gui.DialogueAttributes("Bars"))
	tblRecordList = TblRecordList(tblRecordList,strRecordList)		-- Obtain list of Individual Records to search
	if #tblRecordList >= 200 then
		progbar.Start('Searching Individuals',#tblRecordList)			-- Optionally start the Progress Bar
	end
	for intIndi, ptrIndi in ipairs(tblRecordList) do					-- Check each Individual against each Census
		local isCensusMissing = false
		local intRecordId = fhGetRecordId(ptrIndi)
		local tblDetails = {}
		local strBirthYear = ""
		datBirthDate:SetSimpleDate(fhCallBuiltInFunction("EstimatedBirthDate",ptrIndi,"EARLIEST",9))
		datDeathDate:SetSimpleDate(fhCallBuiltInFunction("EstimatedDeathDate",ptrIndi,"LATEST",9))
		for intEntry, tblCensusData in ipairs(tblCensusData) do		-- Check each Census against life dates of Individual
			datCensusDate = tblCensusData.Date
			if  datBirthDate:Compare(datCensusDate) <= 0
			and datDeathDate:Compare(datCensusDate) >= 0 then			-- Individual is alive at selected Census Date
				-- Check if Census Event is missing and Individual is in Census Country on Census Date
				if IsCensusMissing(ptrIndi,intRecordId,tblCensusData,isInCountry) then -- 25 Nov 2103 V3.0
					isCensusMissing = true										-- Census is Missing and Individual alive in Census Country
					tblDetails = TblIndividualDetails(ptrIndi,intRecordId)
					strBirthYear = tostring(tblDetails.Birth)
					if strBirthYear > "0" then
						strBirthYear = encoder.StrCP1252_XML(strBirthYear.." ± "..tblDetails.Range)	-- 07 June 2013
					end
					ptrFamily, strSurname, ptrFamDate = CurrentFamily(ptrIndi,datCensusDate)
					if strLineColour == "odd" then strLineColour = "even" else strLineColour = "odd" end 
					strWebLine = StrWebLineTemplate							-- Replace web line placeholders with actual components below
					strWebLine = strWebLine:replace("{LineColour}",strLineColour)
					strWebLine = strWebLine:replace("{ItemNumber}",#tblWebGrid + 1)
					strWebLine = strWebLine:replace("{AncestryCo}",StrAncestryCo(strAncestryCo,tblCensusData,ptrIndi,tblDetails,strSurname,ptrFamily))			-- 25 Nov 2013 V3.0
					strWebLine = strWebLine:replace("{FindMyPast}",StrFindMyPast(strFindMyPast,tblCensusData,ptrIndi,tblDetails,strSurname))	 -- 13 June 2013 & 25 Nov 2013 V3.0
					strWebLine = strWebLine:replace("{Individual}",encoder.StrCP1252_XML(strSurname)..", "..encoder.StrGetItem_XML(ptrIndi,"~.NAME:GIVEN_ALL").." ["..intRecordId.."]") -- 07 June 2013
					strWebLine = strWebLine:replace("{Birth_Year}",strBirthYear)
					strWebLine = strWebLine:replace("{Life_Dates}",fhCallBuiltInFunction("LifeDates",ptrIndi))
					strWebLine = strWebLine:replace("{FamilyName}",encoder.StrGetItem_XML(ptrFamily,"~"))				-- 07 June 2013
					strWebLine = strWebLine:replace("{FamilyDate}",encoder.StrGetItem_XML(ptrFamDate,"~:ABBREV2"))	-- 07 June 2013
					strWebLine = strWebLine:replace("{FatherName}",encoder.StrGetItem_XML(ptrIndi,"~.~FATH>"))		-- 07 June 2013
					strWebLine = strWebLine:replace("{MotherName}",encoder.StrGetItem_XML(ptrIndi,"~.~MOTH>"))		-- 07 June 2013
					table.insert(tblWebGrid,strWebLine)
				end
			end
		end
		if isCensusMissing then												-- Census Missing so add Individual to Result Set
			table.insert(tblIndiItem,ptrIndi:Clone())
			table.insert(tblRecordId,intRecordId)
		end
		progbar.Message("Record Id "..intRecordId)
		progbar.Step(1)
		if progbar.Stop() then return end
	end
	if #tblIndiItem > 0 then													-- Invoke browser web page and show Result Set
		strFileName = strFolder..strFile_js
		if not general.FlgFileExists(strFileName) then
			general.SaveStringToFile(StrWebPageJS,strFileName)			-- Output temporary .js Java Script file
		end
		strFileName = strFolder..strFile_css
		if not general.FlgFileExists(strFileName) then
			general.SaveStringToFile(StrWebPageCSS,strFileName)			-- Output temporary .css Style Sheet file
		end
		strWebPage = strWebPage:replace("{WebGrid}",table.concat(tblWebGrid))
		strFileName = strFolder..strFile_html
		general.SaveStringToFile(strWebPage,strFileName)				-- Output & execute temporary .html web page file
		local isOK, intErrorCode, strErrorText = fhShellExecute(strFileName)
		progbar.Step(1)
		fhOutputResultSetTitles(strTitleName, strTitleName, "Printed Date: %#x")
		fhOutputResultSetColumn("Individual" , "item", tblIndiItem, #tblIndiItem, 180, "align_left")
		fhOutputResultSetColumn("Record Id","integer", tblRecordId, #tblIndiItem, 40, "align_right")
		progbar.Close()
	else
		progbar.Close()
		iup_gui.MemoDialogue("\n No "..strTitleName..". \n")			-- No Census Missing informative message
	end
end -- function RunLookup

